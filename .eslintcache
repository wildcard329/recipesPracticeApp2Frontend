th/UserLogin.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/helpers/utils/axiosWithAuth.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboard.jsx",["192","193","194","195","196","197"],"import React, { useEffect, useState } from 'react';\nimport { useLocation, useHistory } from 'react-router-dom';\nimport { useSelector } from 'react-redux';\nimport jwt_decode from 'jwt-decode';\n\nimport { selectUser, selectToken } from '../../model/state/Selector.js';\nimport UserDashboardDisplayUser from './UserDashboardDisplay.jsx';\nimport UserDashboardLogoutDsiplay from './UserDashboardLogoutDsiplay.jsx';\nimport UserHelper from '../../helpers/functions/storageHandler.js';\nimport NavHelper from '../../helpers/functions/locationHandler.js';\nimport UserController from '../../controller/UserController.js';\n\nfunction UserDashboard() {\n    const [display, setDisplay] = useState(false);\n    const token = useSelector(selectToken);\n    const id = UserHelper.getUserId();\n\n    useEffect(() => {\n        token || id ?\n            setDisplay(true)\n        :\n            setDisplay(false)\n    }, [token, id]);\n\n    return(\n        <div>\n            <div className={display ? 'none' : ''}>\n                <UserDashboardLogoutDsiplay />\n            </div>\n            <div className={display ? '' : 'none'}>\n                <UserDashboardDisplayUser />\n            </div>\n        </div>\n    )\n}\n\nexport default UserDashboard;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboardDisplay.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboardLogoutDsiplay.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/helpers/functions/storageHandler.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboardMenu.jsx",["198","199","200"],"import React, { useState } from 'react';\nimport { useSelector } from 'react-redux';\nimport { Dropdown, DropdownButton } from 'react-bootstrap';\n\nimport UserController from '../../controller/UserController';\nimport UserHelper from '../../helpers/functions/storageHandler.js';\nimport { selectUser, selectUserData } from '../../model/state/Selector.js';\nimport { useHistory } from 'react-router-dom';\n\nfunction UserDashboardMenu() {\n    const history = useHistory();\n    const userId = UserHelper.getUserId();\n    const user = useSelector(selectUserData);\n\n    const viewProfile = () => {\n        history.push('/user/profile');\n    }\n\n    const browseRecipes = () => {\n        history.push('/recipes/browse');\n    }\n\n    const browseUserRecipes = async () => {\n        await UserController.getUserData(userId);\n        history.push('/recipes/user');\n    }\n\n    const toAddRecipe = () => {\n        history.push('/recipes/add');\n    };\n\n    const logout = async () => {\n        UserHelper.removeTokenSession();\n        UserController.logoutUser(user);\n        history.push('/auth/login');\n    };\n\n    return(\n        <div className='user-dashboard-left'>\n            <Dropdown>\n                <Dropdown.Toggle variant=\"primary\" id=\"dropdown-basic\">\n                    Menu\n                </Dropdown.Toggle>\n                <Dropdown.Menu>\n                    <Dropdown.Item onClick={viewProfile}>Profile</Dropdown.Item>\n                    <Dropdown.Item onClick={browseRecipes}>Browse</Dropdown.Item>\n                    <Dropdown.Item onClick={browseUserRecipes}>Cookbook</Dropdown.Item>\n                    <Dropdown.Item onClick={toAddRecipe}>New Recipe</Dropdown.Item>\n                    <Dropdown.Item onClick={logout}>Logout</Dropdown.Item>\n                </Dropdown.Menu>\n            </Dropdown>\n        </div>\n    )\n}\n\nexport default UserDashboardMenu;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboardUser.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserProfile.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/MultiRecipeList.jsx",["201"],"import React, { useEffect } from \"react\";\nimport { useSelector } from 'react-redux';\nimport { useHistory } from 'react-router-dom';\n\nimport RecipeController from '../../controller/RecipeController.js';\nimport UserController from \"../../controller/UserController.js\";\nimport { selectSampleRecipes } from '../../model/state/Selector.js';\nimport UserHelper from '../../helpers/functions/storageHandler.js';\n\nimport RecipeMapper from \"./RecipeMapper.jsx\";\n\nfunction RecipeList() {\n    const history = useHistory();\n    const userId = UserHelper.getUserId();\n    const samples = useSelector(selectSampleRecipes);\n    const filters = samples && samples[0];\n    const recipes = samples && samples[1];\n    const userRecipes = samples && samples[2];\n    const filter1 = samples && samples[3];\n    const filter2 = samples && samples[4];\n    const filter3 = samples && samples[5];\n   \n    const setUserRecipes = () => {\n        UserController.getUserData(userId);\n    };\n    \n    const toAllRecipes = () => {\n        history.push('/recipes/all');\n    }\n    \n    const toRecipeList = () => {\n        history.push('/recipes/user');\n    }\n    \n    useEffect(async () => {\n        await RecipeController.browseRecipes(userId);\n    }, [userId]);\n\n    return(\n        <div>\n            <div className='recipe-lists'>\n                <h2 className='list-header' onClick={toAllRecipes}>Browse all recipes</h2>\n                <RecipeMapper recipes={recipes} />\n            </div>\n            <div className='recipe-lists' onClick={setUserRecipes}>\n                <h2 className='list-header' onClick={toRecipeList}>Your Recipes</h2>\n                <RecipeMapper recipes={userRecipes} />\n            </div>\n            <div className='recipe-lists' onClick={null}>\n                <h2 className='list-header' onClick={null}>Try {filters && filters[0]} Recipes</h2>\n                <RecipeMapper recipes={filter1} />\n            </div>\n            <div className='recipe-lists' onClick={null}>\n                <h2 className='list-header' onClick={null}>Try {filters && filters[1]} Recipes</h2>\n                <RecipeMapper recipes={filter2} />\n            </div>\n            <div className='recipe-lists' onClick={null}>\n                <h2 className='list-header' onClick={null}>Try {filters && filters[2]} Recipes</h2>\n                <RecipeMapper recipes={filter3} />\n            </div>\n        </div>\n    )\n}\n\nexport default RecipeList;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/SingleRecipeList.jsx",["202","203"],"import React, { useEffect, useState } from 'react';\nimport { useSelector } from 'react-redux';\nimport { useLocation } from 'react-router-dom';\n\nimport RecipeController from '../../controller/RecipeController.js';\nimport { selectRecipeList, selectUserData, selectUserRecipeList } from '../../model/state/Selector.js';\nimport RecipeMapper from './RecipeMapper.jsx';\n\nfunction SingleRecipeList() {\n    const id = useSelector(selectUserData).id;\n    const recipes = useSelector(selectRecipeList);\n    const userRecipes = useSelector(selectUserRecipeList);\n    const path = useLocation().pathname;\n    const [list, setList] = useState(null);\n    \n    useEffect(async () => {\n        switch (path) {\n            case '/recipes/all':\n                await RecipeController.getRecipeList();\n                setList(recipes)\n                break;\n            case '/recipes/user':\n                await RecipeController.getUserRecipeList(id);\n                setList(userRecipes);\n                break;    \n            default:\n                return;\n            }\n        }, [id, list]);\n        \n    return(\n        <div className='single-list'>\n            <RecipeMapper recipes={list} />\n        </div>\n    )\n}\n\nexport default SingleRecipeList;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/helpers/functions/locationHandler.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/routes/AppRoute.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/routes/AppNavigator.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/auth/AuthError.jsx",[],["204","205"],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/helpers/functions/formFunctionHandler.js",["206","207"],"class formFunctions {\n    static convertArrToHtml(inputArr) {\n        // this maps an array to html objects\n        const outputArr = inputArr.map(item => {\n            return {\n                htmlId: inputArr?.htmlId || this.generateId(),\n                name: item?.name || '',\n                recipe: item?.recipe || null\n            };\n        });\n        return outputArr;\n    };\n    static editListItem(item, value) {\n        // this sets an instruction or ingredient before it is passed to the reducer\n        return {\n            htmlId: item.htmlId,\n            name: value,\n            recipe: item.recipe\n        }\n    };\n    static setListItem(item, list) {\n        // this enables editing of the html, which if set in the component would have to use every key from the ingredient object when only the name key is needed\n        const newList = list.map(listItem => \n            listItem.htmlId !== item.htmlId ? listItem : item\n        );\n        return newList\n    };\n    static removeListItem(item, list) {\n        // this removes a list element from the html and from state\n        const filteredList = list.filter(listItem => {\n            return listItem.htmlId !== item.htmlId\n        });\n        return filteredList;\n    };\n    static generateId() {\n        // this generates a unique id for html elements\n        return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function(c) {\n            const r = Math.random() * 16 | 0, v = c === 'x' ? r : (r & 0x3 | 0x8);\n            return v.toString(16);\n        })\n    }\n};\n\nexport default formFunctions;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/SearchRecipes.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/RecipeForm.jsx",["208","209","210","211","212","213","214"],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/RecipeFormInstructions.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/RecipeFormIngredients.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/controller/FormController.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/RecipeMapper.jsx",[],{"ruleId":"215","replacedBy":"216"},{"ruleId":"217","replacedBy":"218"},{"ruleId":"219","severity":1,"message":"220","line":2,"column":10,"nodeType":"221","messageId":"222","endLine":2,"endColumn":20},{"ruleId":"219","severity":1,"message":"223","line":6,"column":28,"nodeType":"221","messageId":"222","endLine":6,"endColumn":49},{"ruleId":"219","severity":1,"message":"224","line":6,"column":51,"nodeType":"221","messageId":"222","endLine":6,"endColumn":73},{"ruleId":"219","severity":1,"message":"225","line":6,"column":75,"nodeType":"221","messageId":"222","endLine":6,"endColumn":85},{"ruleId":"226","severity":1,"message":"227","line":22,"column":15,"nodeType":"228","endLine":30,"endColumn":6},{"ruleId":"226","severity":1,"message":"229","line":30,"column":8,"nodeType":"230","endLine":30,"endColumn":10,"suggestions":"231"},{"ruleId":"219","severity":1,"message":"232","line":1,"column":16,"nodeType":"221","messageId":"222","endLine":1,"endColumn":25},{"ruleId":"219","severity":1,"message