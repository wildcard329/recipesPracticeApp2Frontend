ect } from 'react-router-dom';\n\nconst PrivateRoute = ({ component: Component, ...rest}) => {\n    return(\n        <Route\n            {...rest}\n            render={props => {\n                if (localStorage.getItem('token')) {\n                    return <Component {...props} />;\n                } else {\n                    return <Redirect to='/auth/login' />;\n                }\n            }}\n        />\n    )\n};\n\nexport default PrivateRoute;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/auth/UserRegister.jsx",["209"],"import React, { useState } from 'react';\nimport { Form, Button } from 'react-bootstrap';\n\nimport UserController from '../../controller/UserController.js';\nimport AuthError from './AuthError.jsx';\n\nfunction UserRegister() {\n    const [user, setUser] = useState({\n        username: '',\n        password: '',\n        email: null,\n        created_on: new Date()\n    });\n\n    const enterUser = e => {\n        setUser({...user, [e.target.name]: e.target.value});\n    };\n\n    const submitRegistration = e => {\n        e.preventDefault();\n        UserController.registerNewUser(user);\n        UserController.routeToDestination('login');\n    };\n\n    const cancelRegistration = () => {\n        UserController.routeToDestination('login');\n    }\n\n    return(\n        <div className='auth-form'>\n            <Form>\n                <Form.Group controlId='username'>\n                    <input id='username' type='text' placeholder='username' onChange={enterUser} />\n                </Form.Group>\n                <Form.Group controlId='password'>\n                    <input id='password' type='password' placeholder='password' onChange={enterUser} />\n                </Form.Group>\n                <Form.Group controlId='email'>\n                    <input id='email' type='email' placeholder='email' onChange={enterUser} />\n                </Form.Group>\n                <Form.Group className='auth-btn-group'>\n                    <Button className='btn btn-primary' onClick={submitRegistration}>Submit</Button>\n                    <Button className='btn btn-secondary' onClick={cancelRegistration}>Cancel</Button>\n                </Form.Group>\n                {/* <AuthError /> */}\n            </Form>\n        </div>\n    )\n}\n\nexport default UserRegister;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/auth/UserLogin.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/helpers/utils/axiosWithAuth.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboard.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboardDisplay.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboardLogoutDsiplay.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/helpers/functions/storageHandler.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboardMenu.jsx",["210","211"],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboardUser.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserProfile.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/MultiRecipeList.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/SingleRecipeList.jsx",["212"],"import React, { useEffect, useState } from 'react';\nimport { useSelector } from 'react-redux';\nimport { useLocation } from 'react-router-dom';\n\nimport RecipeController from '../../controller/RecipeController.js';\nimport { selectRecipeList, selectUserData, selectUserRecipeList } from '../../model/state/Selector.js';\nimport RecipeCard from './RecipeCard.jsx';\n\nfunction SingleRecipeList() {\n    const id = useSelector(selectUserData).id;\n    const recipes = useSelector(selectRecipeList);\n    const userRecipes = useSelector(selectUserRecipeList);\n    const path = useLocation().pathname;\n    const [list, setList] = useState(null);\n\n    useEffect(() => {\n        switch (path) {\n            case '/recipes/all':\n                setList('all')\n                return RecipeController.getRecipeList();\n            case '/recipes/user':\n                setList('user');\n                return RecipeController.getUserRecipeList(id);\n            default:\n                return;\n            }\n    }, [id]);\n    return(\n        <div>\n            {list === 'all' ? \n            <div>\n                <RecipeCard recipes={recipes} />\n            </div>\n            : list === 'user' ?\n            <div>\n                <RecipeCard recipes={userRecipes} />\n            </div>\n            :\n            null\n            }\n        </div>\n    )\n}\n\nexport default SingleRecipeList;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/helpers/functions/locationHandler.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/routes/AppRoute.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/routes/AppNavigator.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/auth/AuthError.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/formComponents/addRecipe/AddRecipeName.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/formComponents/addRecipe/AddRecipeDescription.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/formComponents/addRecipe/AddRecipeIngredients.jsx",[],["213","214"],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/formComponents/addRecipe/AddRecipeInstructions.jsx",[],["215","216"],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src