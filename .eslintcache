       });\n            reader.readAsDataURL(e.target.files[0])\n        }\n    }\n\n    const handleFile = e => {\n        e.preventDefault();\n        setFile(e.target.files[0]);\n        setFilename(e.target.files[0].name);\n        const reader = new FileReader();\n        reader.addEventListener(\"load\", () => {\n            setFile(reader.result);\n        });\n        reader.readAsDataURL(e.target.files[0]);\n        console.log('file: ',file);\n    }\n    \n    const cancel = e => {\n        e.preventDefault();\n        history.push('/recipes/all');\n    }\n    \n    const submitRecipe = e => {\n        const data = new FormData();\n        data.append('name', recipe.name);\n        data.append('description', recipe.description);\n        data.append('author', recipe.author)\n        data.append('filename', filename);\n        data.append('file', file);\n        e.preventDefault();\n        console.log('Sending :',data)\n        RecipeController.addRecipeData(data)\n    }\n    return(\n        <div>\n            <form onSubmit={submitRecipe}>\n                <div>\n                    <label htmlFor='name'>Recipe</label>\n                    <input id='name' type='text' name='name' onChange={handleRecipe} />\n                </div>\n                <div>\n                    <label htmlFor='description'>Description</label>\n                    <input id='description' type='text' name='description' onChange={handleRecipe} />\n                </div>\n                <div>\n                    <label htmlFor='image'>Picture</label>\n                    <input id='image' type='file' name='image' onChange={handleFile} />\n                </div>\n                <div>\n                    <button onClick={cancel}>Cancel</button>\n                    <button onClick={submitRecipe}>Submit</button>\n                </div>\n            </form>\n            <image src ={file} />\n            <RecipePreview recipe={recipe} file={file} filename={filename} />\n        </div>\n    )\n}\n\nexport default CreateRecipe;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/RecipeCard.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/controller/RecipeController.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/model/services/RecipeServices.js",["132"],"import axios from 'axios';\nimport axiosWithAuth from '../../helpers/utils/axiosWithAuth.js';\n\nconst RecipesURL = new URL('http://localhost:5000/api/recipes')\n\nclass RecipeServices {\n    async getRecipeList() {\n        const response = await axiosWithAuth.get(`${RecipesURL}/all`);\n        return response.data;\n    };\n    async getRecipeData(id) {\n        const response = await axiosWithAuth.get(`${RecipesURL}/${id}`);\n        return response.data;\n    };\n    async addRecipeData(recipe) {\n        await axiosWithAuth.post(`${RecipesURL}/create`, recipe, { headers: { 'Content-Type': 'multipart/form-data'}});\n    };\n};\n\nexport default new RecipeServices();\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/ViewRecipeDetails.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/model/state/reducer.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/model/state/Action.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/model/state/Selector.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/testimageupload.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/RecipePreview.jsx",["133"],"import React, { useState } from 'react';\n\nfunction  RecipePreview(props) {\n    console.log('props: ', props)\n\n    return(\n        <div>\n            <h1>Preview</h1>\n            <image src={'/na.jpeg'} alt='image-not-available' style={{width: '500px'}} />\n            <h2>{props.recipe.name}</h2>\n            <p>{props.recipe.author}</p>\n        </div>\n    )\n}\n\nexport default RecipePreview;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/routes/PrivateRoute.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/auth/UserRegister.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/auth/UserLogin.jsx",["134"],"import React, { useState } from 'react';\nimport UserController from '../../controller/UserController';\nimport { useHistory, link } from 'react-router-dom';\n\nfunction UserLogin() {\n    const [user, setUser] = useState({\n        username: '',\n        password: ''\n    })\n    const history = useHistory();\n\n    const onChangeCredentials = e => {\n        setUser({...user, [e.target.name]: e.target.value});\n    }\n\n    const submitLogin = e => {\n        e.preventDefault();\n        UserController.loginUser(user);\n        history.push('/recipes/all');\n    }\n\n    const goToRegister = () => {\n        history.push('/auth/register')\n    }\n    return(\n        <div>\n            <form onSubmit={submitLogin}>\n                <div>\n                    <label htmlFor='username'>Enter username</label>\n                    <input id='username' type='text' name='username' onChange={onChangeCredentials} />\n                </div>\n                <div>\n                    <label htmlFor='password'>Enter password</label>\n                    <input id='password' type='text' name='password' onChange={onChangeCredentials} />\n                </div>\n                <div>\n                    <button onClick={submitLogin}>Login</button>\n                </div>\n            </form>\n            <div>\n                <h4>Don't have an account?</h4>\n                <button onClick={goToRegister}>Register</button>\n            </div>\n        </div>\n    )\n}\n\nexport default UserLogin;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/helpers/utils/axiosWithAuth