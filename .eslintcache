 from 'react';\nimport { useHistory } from 'react-router-dom';\nimport UserController from '../../controller/UserController';\n\nfunction CreateUser() {\n    const [user, setUser] = useState({\n        username: '',\n        password: '',\n        email: ''\n    });\n\n    const enterUser = e => {\n        e.preventDefault();\n        setUser({...user, [e.target.name]: e.target.value});\n    };\n\n    const cancel = () => {\n        UserController.routeToDestination('all users');\n    };\n\n    const addUser = async () => {\n        await UserController.addNewUser(user);\n        UserController.routeToDestination('all users');\n    };\n\n    return(\n        <div>\n            <form onSubmit={addUser}>\n                <div>\n                    <label htmlFor='username'>Username</label>\n                    <input id='username' type='text' name='username' onChange={enterUser} />\n                </div>\n                <div>\n                    <label htmlFor='password'>Password</label>\n                    <input id='password' type='text' name='password' onChange={enterUser} />\n                </div>\n                <div>\n                    <label htmlFor='email'>Email</label>\n                    <input id='email' type='text' name='email' onChange={enterUser} />\n                </div>\n                <div>\n                    <button onClick={cancel}>Cancel</button>\n                    <button onClick={addUser}>Submit</button>\n                </div>\n            </form>\n        </div>\n    )\n}\n\nexport default CreateUser;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserInfo.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserCard.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/store.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/controller/UserController.js",[],"/home/wildcard/Programming/Javascript/React/re