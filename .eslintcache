sers/CreateUser.jsx",["207"],"import React, { useState } from 'react';\nimport { useHistory } from 'react-router-dom';\nimport UserController from '../../controller/UserController';\n\nfunction CreateUser() {\n    const [user, setUser] = useState({\n        username: '',\n        password: '',\n        email: ''\n    });\n\n    const enterUser = e => {\n        e.preventDefault();\n        setUser({...user, [e.target.name]: e.target.value});\n    };\n\n    const cancel = () => {\n        UserController.routeToDestination('all users');\n    };\n\n    const addUser = async () => {\n        await UserController.addNewUser(user);\n        UserController.routeToDestination('all users');\n    };\n\n    return(\n        <div>\n            <form onSubmit={addUser}>\n                <div>\n                    <label htmlFor='username'>Username</label>\n                    <input id='username' type='text' name='username' onChange={enterUser} />\n                </div>\n                <div>\n                    <label htmlFor='password'>Password</label>\n                    <input id='password' type='text' name='password' onChange={enterUser} />\n                </div>\n                <div>\n                    <label htmlFor='email'>Email</label>\n                    <input id='email' type='text' name='email' onChange={enterUser} />\n                </div>\n                <div>\n                    <button onClick={cancel}>Cancel</button>\n                    <button onClick={addUser}>Submit</button>\n                </div>\n            </form>\n        </div>\n    )\n}\n\nexport default CreateUser;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserInfo.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserCard.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/store.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/controller/UserController.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/model/services/UserServices.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/ViewUserDetails.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/RecipeInfo.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/RecipeCard.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/controller/RecipeController.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/model/services/RecipeServices.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/ViewRecipeDetails.jsx",["208"],"import React from 'react';\nimport { Card } from 'react-bootstrap';\nimport { useHistory } from 'react-router-dom';\n\nimport UserController from '../../controller/UserController.js';\nimport RecipeController from '../../controller/RecipeController.js';\n\nfunction ViewRecipeDetails({recipe}) {\n    const data = recipe.image\n    const recipeId = recipe.id\n    const history = useHistory();\n\n    const getRecipeData = async e => {\n        e.preventDefault();\n        await RecipeController.getRecipeData(recipe.id);\n        history.push(`/recipe/${recipeId}/info`);\n        // UserController.routeToDestination('recipe info', recipeId);\n    };\n\n    return(\n        <Card key={recipe.id} onClick={getRecipeData} className='recipe-card'>\n            <Card.Img src={`data:image/jpeg;base64,${data}`} alt='recipe image' />\n            <Card.Title>{recipe.name}</Card.Title>\n        </Card>\n    )\n}\n\nexport default ViewRecipeDetails;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/model/state/reducer.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/model/state/Action.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/model/state/Selector.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/routes/PrivateRoute.jsx",["209"],"import React, {Component} from 'react';\nimport { Route, Redirect } from 'react-router-dom';\n\nconst PrivateRoute = ({ component: Component, ...rest}) => {\n    return(\n        <Route\n            {...rest}\n            render={props => {\n                if (localStorage.getItem('token')) {\n                    return <Component {...props} />;\n                } else {\n                    return <Redirect to='/auth/login' />;\n                }\n            }}\n        />\n    )\n};\n\nexport default PrivateRoute;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/auth/UserRegister.jsx",["210"],"import React, { useState } from 'react';\nimport { Form, Button } from 'react-bootstrap';\n\nimport UserController from '../../controller/UserController.js';\nimport AuthError from './AuthError.jsx';\n\nfunction UserRegister() {\n    const [user, setUser] = useState({\n        username: '',\n        password: '',\n        email: null,\n        created_on: new Date()\n    });\n\n    const enterUser = e => {\n        setUser({...user, [e.target.name]: e.target.value});\n    };\n\n    const submitRegistration = e => {\n        e.preventDefault();\n        UserController.registerNewUser(user);\n        UserController.routeToDestination('login');\n    };\n\n    const cancelRegistration = () => {\n        UserController.routeToDestination('login');\n    }\n\n    return(\n        <div className='auth-form'>\n            <Form>\n                <Form.Group controlId='username'>\n                    <input id='username' type='text' placeholder='username' onChange={enterUser} />\n                </Form.Group>\n                <Form.Group controlId='password'>\n                    <input id='password' type='password' placeholder='password' onChange={enterUser} />\n                </Form.Group>\n                <Form.Group controlId='email'>\n                    <input id='email' type='email' placeholder='email' onChange={enterUser} />\n                </Form.Group>\n                <Form.Group className='auth-btn-group'>\n                    <Button className='btn btn-primary' onClick={submitRegistration}>Submit</Button>\n                    <Button className='btn btn-secondary' onClick={cancelRegistration}>Cancel</Button>\n                </Form.Group>\n                {/* <AuthError /> */}\n            </Form>\n        </div>\n    )\n}\n\nexport default UserRegister;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/auth/UserLogin.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/helpers/utils/axiosWithAuth.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboard.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboardDisplay.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboardLogoutDsiplay.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/helpers/functions/storageHandler.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboardMenu.jsx",["211","212"],"import React, { useState } from 'react';\nimport { useSelector } from 'react-redux';\nimport { Dropdown, DropdownButton } from 'react-bootstrap';\n\nimport UserController from '../../controller/UserController';\nimport UserHelper from '../../helpers/functions/storageHandler.js';\nimport { selectUser } from '../../model/state/Selector.js';\n\nfunction UserDashboardMenu() {\n    const user = useSelector(selectUser);\n\n    const viewProfile = () => {\n        UserController.routeToDestination('profile');\n    }\n\n    const browseRecipes = () => {\n        UserController.routeToDestination('browse');\n    }\n\n    const browseUserRecipes = async () => {\n        await UserController.getUserData(user.id);\n        UserController.routeToDestination('user recipes');\n    }\n\n    const toAddRecipe = () => {\n        UserController.routeToDestination('add recipe');\n    };\n\n    const logout = async () => {\n        UserHelper.removeTokenSession();\n        UserController.logoutUser(user);\n        UserController.routeToDestination('login');\n    };\n\n    return(\n        <div className='user-dashboard-left'>\n            <Dropdown>\n                <Dropdown.Toggle variant=\"primary\" id=\"dropdown-basic\">\n                    Menu\n                </Dropdown.Toggle>\n                <Dropdown.Menu>\n                    <Dropdown.Item onClick={viewProfile}>Profile</Dropdown.Item>\n                    <Dropdown.Item onClick={browseRecipes}>Browse</Dropdown.Item>\n                    <Dropdown.Item onClick={browseUserRecipes}>Cookbook</Dropdown.Item>\n                    <Dropdown.Item onClick={toAddRecipe}>New Recipe</Dropdown.Item>\n                    <Dropdown.Item onClick={logout}>Logout</Dropdown.Item>\n                </Dropdown.Menu>\n            </Dropdown>\n        </div>\n    )\n}\n\nexport default UserDashboardMenu;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboardUser.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserProfile.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/MultiRecipeList.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/SingleRecipeList.jsx",["213"],"import React, { useEffect, useState } from 'react';\nimport { useSelector } from 'react-redux';\nimport { useLocation } from 'react-router-dom';\n\nimport RecipeController from '../../controller/RecipeController.js';\nimport { selectRecipeList, selectUserData, selectUserRecipeList } from '../../model/state/Selector.js';\nimport RecipeCard from './RecipeCard.jsx';\n\nfunction SingleRecipeList() {\n    const id = useSelector(selectUserData).id;\n    const recipes = useSelector(selectRecipeList);\n    const userRecipes = useSelector(selectUserRecipeList);\n    const path = useLocation().pathname;\n    const [list, setList] = useState(null);\n\n    useEffect(() => {\n        switch (path) {\n            case '/recipes/all':\n                setList('all')\n                return RecipeController.getRecipeList();\n            case '/recipes/user':\n                setList('user');\n                return RecipeController.getUserRecipeList(id);\n            default:\n                return;\n            }\n    }, [id]);\n    return(\n        <div>\n            {list === 'all' ? \n            <div>\n                <RecipeCard recipes={recipes} />\n            </div>\n            : list === 'user' ?\n            <div>\n                <RecipeCard recipes={userRecipes} />\n            </div>\n            :\n            null\n            }\n        </div>\n    )\n}\n\nexport default SingleRecipeList;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/helpers/functions/locationHandler.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/routes/AppRoute.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/routes/AppNavigator.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/auth/AuthError.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/formComponents/addRecipe/AddRecipeName.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/formComponents/addRecipe/AddRecipeDescription.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/formComponents/addRecipe/AddRecipeIngredients.jsx",[],["214","215"],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/formComponents/addRecipe/AddRecipeInstructions.jsx",[],["216","217"],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/formComponents/formError/RequiredField.jsx",[],["218","219"],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/helpers/functions/formFunctionHandler.js",["220","221"],"class formFunctions {\n    static convertArrToHtml(inputArr) {\n        // this maps an array to html objects\n        const outputArr = inputArr.map(item => {\n            return {\n                htmlId: inputArr?.htmlId || this.generateId(),\n                name: item?.name || '',\n                recipe: item?.recipe || null\n            };\n        });\n        return outputArr;\n    };\n    static editListItem(item, value) {\n        // this sets an instruction or ingredient before it is passed to the reducer\n        return {\n            htmlId: item.htmlId,\n            name: value,\n            recipe: item.recipe\n        }\n    };\n    static setListItem(item, list) {\n        // this enables editing of the html, which if set in the component would have to use every key from the ingredient object when only the name key is needed\n        const newList = list.map(listItem => \n            listItem.htmlId !== item.htmlId ? listItem : item\n        );\n        return newList\n    };\n    static removeListItem(item, list) {\n        // this removes a list element from the html and from state\n        const filteredList = list.filter(listItem => {\n            return listItem.htmlId !== item.htmlId\n        });\n        return filteredList;\n    };\n    static generateId() {\n        // this generates a unique id for html elements\n        return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function(c) {\n            const r = Math.random() * 16 | 0, v = c === 'x' ? r : (r & 0x3 | 0x8);\n            return v.toString(16);\n        })\n    }\n};\n\nexport default formFunctions;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/SearchRecipes.jsx",[],"/home/wildcard/Programming/Javascript/Rea