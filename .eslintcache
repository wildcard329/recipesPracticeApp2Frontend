erInfo.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserCard.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/store.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/controller/UserController.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/model/services/UserServices.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/ViewUserDetails.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/RecipeInfo.jsx",["203"],"import React from 'react';\nimport { useSelector } from 'react-redux';\n\nimport { selectRecipeData } from '../../model/state/Selector.js';\nimport UserController from '../../controller/UserController.js';\n\nfunction RecipeInfo() {\n    const recipe = useSelector(selectRecipeData);\n    const data = recipe.image;\n\n    const toRecipes = e => {\n        e.preventDefault();\n        UserController.routeToDestination('browse');\n    };\n\n    return(\n        <div className='single-recipe-card'>\n            <h2>{recipe.name}</h2>\n            <img src={`data:image/jpeg;base64,${data}`} /> \n            <p>{recipe.description}</p>\n            <p>Posted by {recipe.author}</p>\n            <button onClick={toRecipes}>Recipes</button>\n        </div>\n    )\n}\n\nexport default RecipeInfo;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/EditRecipe.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/CreateRecipe.jsx",["204","205","206"],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/RecipeCard.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/controller/RecipeController.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/model/services/RecipeServices.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/ViewRecipeDetails.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/model/state/reducer.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/model/state/Action.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/model/state/Selector.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/routes/PrivateRoute.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/auth/UserRegister.jsx",["207"],"import React, { useState } from 'react';\nimport { Form, Button } from 'react-bootstrap';\n\nimport UserController from '../../controller/UserController.js';\nimport AuthError from './AuthError.jsx';\n\nfunction UserRegister() {\n    const [user, setUser] = useState({\n        username: '',\n        password: '',\n        email: null,\n        created_on: new Date()\n    });\n\n    const enterUser = e => {\n        setUser({...user, [e.target.name]: e.target.value});\n    };\n\n    const submitRegistration = e => {\n        e.preventDefault();\n        UserController.registerNewUser(user);\n        UserController.routeToDestination('login');\n    };\n\n    const cancelRegistration = () => {\n        UserController.routeToDestination('login');\n    }\n\n    return(\n        <div className='auth-form'>\n            <Form>\n                <Form.Group controlId='username'>\n                    <input id='username' type='text' placeholder='username' onChange={enterUser} />\n                </Form.Group>\n                <Form.Group controlId='password'>\n                    <input id='password' type='password' placeholder='password' onChange={enterUser} />\n                </Form.Group>\n                <Form.Group controlId='email'>\n                    <input id='email' type='email' placeholder='email' onChange={enterUser} />\n                </Form.Group>\n                <Form.Group className='auth-btn-group'>\n                    <Button className='btn btn-primary' onClick={submitRegistration}>Submit</Button>\n                    <Button className='btn btn-secondary' onClick={cancelRegistration}>Cancel</Button>\n                </Form.Group>\n                {/* <AuthError /> */}\n            </Form>\n        </div>\n    )\n}\n\nexport default UserRegister;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/auth/UserLogin.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/helpers/utils/axiosWithAuth.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboard.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboardDisplay.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboardLogoutDsiplay.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/helpers/functions/storageHandler.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboardMenu.jsx",["208","209","210"],"import React, { useState } from 'react';\nimport { useSelector } from 'react-redux';\nimport { Dropdown, DropdownButton } from 'react-bootstrap';\n\nimport UserController from '../../controller/UserController';\nimport UserHelper from '../../helpers/functions/storageHandler.js';\nimport { selectUser } from '../../model/state/Selector.js';\n\nfunction UserDashboardMenu() {\n    const user = useSelector(selectUser);\n    const [open, setOpen] = useState(false);\n\n    const openMenu = e => {\n        e.preventDefault();\n        setOpen(true);\n    };\n    \n    const closeMenu = () => {\n        setOpen(false)\n    }\n\n    const viewProfile = () => {\n        UserController.routeToDestination('profile');\n        closeMenu();\n    }\n\n    const browseRecipes = () => {\n        UserController.routeToDestination('browse');\n        closeMenu();\n    }\n\n    const browseUserRecipes = () => {\n        UserController.getUserData(user.id);\n        UserController.routeToDestination('user recipes');\n        closeMenu();\n    }\n\n    const toAddRecipe = () => {\n        UserController.routeToDestination('add recipe');\n        closeMenu();\n    };\n\n    const logout = () => {\n        UserHelper.removeTokenSession();\n        UserController.logoutUser(user);\n        UserController.routeToDestination('login');\n        closeMenu();\n    };\n\n    return(\n        <div className='user-dashboard-left'>\n            <Dropdown>\n                <Dropdown.Toggle variant=\"primary\" id=\"dropdown-basic\">\n                    Menu\n                </Dropdown.Toggle>\n                <Dropdown.Menu>\n                    <Dropdown.Item onClick={viewProfile}>Profile</Dropdown.Item>\n                    <Dropdown.Item onClick={browseRecipes}>Browse</Dropdown.Item>\n                    <Dropdown.Item onClick={browseUserRecipes}>Cookbook</Dropdown.Item>\n                    <Dropdown.Item onClick={toAddRecipe}>New Recipe</Dropdown.Item>\n                    <Dropdown.Item onClick={logout}>Logout</Dropdown.Item>\n                </Dropdown.Menu>\n            </Dropdown>\n        </div>\n    )\n}\n\nexport default UserDashboardMenu;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboardUser.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserProfile.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/MultiRecipeList.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/SingleRecipeList.jsx",["211"],"import React, { useEffect, useState } from 'react';\nimport { useSelector } from 'react-redux';\nimport { useLocation } from 'react-router-dom';\n\nimport RecipeController from '../../controller/RecipeController.js';\nimport { selectRecipeList, selectUserData, select