nd2/src/view/users/CreateUser.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserInfo.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserCard.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/store.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/controller/UserController.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/model/services/UserServices.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/ViewUserDetails.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/RecipeInfo.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/RecipeCard.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/controller/RecipeController.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/model/services/RecipeServices.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/ViewRecipeDetails.jsx",["208"],"import React from 'react';\nimport { Card } from 'react-bootstrap';\nimport { useHistory } from 'react-router-dom';\n\nimport UserController from '../../controller/UserController.js';\nimport RecipeController from '../../controller/RecipeController.js';\n\nfunction ViewRecipeDetails({recipe}) {\n    const data = recipe.image\n    const recipeId = recipe.id\n    const history = useHistory();\n\n    const getRecipeData = async e => {\n        e.preventDefault();\n        await RecipeController.getRecipeData(recipe.id);\n        history.push(`/recipe/${recipeId}/info`);\n        // UserController.routeToDestination('recipe info', recipeId);\n    };\n\n    return(\n        <Card key={recipe.id} onClick={getRecipeData} className='recipe-card'>\n            <Card.Img src={`data:image/jpeg;base64,${data}`} alt='recipe image' />\n            <Card.Title>{recipe.name}</Card.Title>\n        </Card>\n    )\n}\n\nexport default ViewRecipeDetails;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/model/state/reducer.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/model/state/Action.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/model/state/Selector.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/routes/PrivateRoute.jsx",["209"],"import React, {Component} from 'react';\nimport { Route, Redirect } from 'react-router-dom';\n\nconst PrivateRoute = ({ component: Component, ...rest}) => {\n    return(\n        <Route\n            {...rest}\n            render={props => {\n                if (localStorage.getItem('token')) {\n                    return <Component {...props} />;\n                } else {\n                    return <Redirect to='/auth/login' />;\n                }\n            }}\n        />\n    )\n};\n\nexport default PrivateRoute;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/auth/UserRegister.jsx",["210"],"import React, { useState } from 'react';\nimport { Form, Button } from 'react-bootstrap';\n\nimport UserController from '../../controller/UserController.js';\nimport AuthError from './AuthError.jsx';\n\nfunction UserRegister() {\n    const [user, setUser] = useState({\n        username: '',\n        password: '',\n        email: null,\n        created_on: new Date()\n    });\n\n    const enterUser = e => {\n        setUser({...user, [e.target.name]: e.target.value});\n    };\n\n    const submitRegistration = e => {\n        e.preventDefault();\n        UserController.registerNewUser(user);\n        UserController.routeToDestination('login');\n    };\n\n    const cancelRegistration = () => {\n        UserController.routeToDestination('login');\n    }\n\n    return(\n        <div className='auth-form'>\n            <Form>\n                <Form.Group controlId='username'>\n                    <input id='username' type='text' placeholder='username' onChange={enterUser} />\n                </Form.Group>\n                <Form.Group controlId='password'>\n                    <input id='password' type='password' placeholder='password' onChange={enterUser} />\n                </Form.Group>\n                <Form.Group controlId='email'>\n                    <input id='email' type='email' placeholder='email' onChange={enterUser} />\n                </Form.Group>\n                <Form.Group className='auth-btn-group'>\n                    <Button className='btn btn-primary' onClick={submitRegistration}>Submit</Button>\n                    <Button className='btn btn-secondary' onClick={cancelRegistration}>Cancel</Button>\n                </Form.Group>\n                {/* <AuthError /> */}\n            </Form>\n        </div>\n    )\n}\n\nexport default UserRegister;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/auth/UserLogin.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/helpers/utils/axiosWithAuth.js",[],["211","212"],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboard.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboardDisplay.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboardLogoutDsiplay.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/helpers/functions/storageHandler.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboardMenu.jsx",["213","214"],"import React, { useState } from 'react';\nimport { useSelector } from 'react-redux';\nimport { Dropdown, DropdownButton } from 'react-bootstrap';\n\nimport UserController from '../../controller/UserController';\nimport UserHelper from '../../helpers/functions/storageHandler.js';\nimport { selectUser } from '../../model/state/Selector.js';\n\nfunction UserDashboardMenu() {\n    const user = useSelector(selectUser);\n\n    const viewProfile = () => {\n        UserController.routeToDestination('profile');\n    }\n\n    const browseRecipes = () => {\n        UserController.routeToDestination('browse');\n    }\n\n    const browseUserRecipes = async () => {\n        await UserController.getUserData(user.id);\n        UserController.routeToDestination('user recipes');\n    }\n\n    const toAddRecipe = () => {\n        UserController.routeToDestination('add recipe');\n    };\n\n    const logout = async () => {\n        UserHelper.removeTokenSession();\n        UserController.logoutUser(user);\n        UserController.routeToDestination('login');\n    };\n\n    return(\n        <div className='user-dashboard-left'>\n            <Dropdown>\n                <Dropdown.Toggle variant=\"primary\" id=\"dropdown-basic\">\n                    Menu\n                </Dropdown.Toggle>\n                <Dropdown.Menu>\n                    <Dropdown.Item onClick={viewProfile}>Profile</Dropdown.Item>\n                    <Dropdown.Item onClick={browseRecipes}>Browse</Dropdown.Item>\n                    <Dropdown.Item onClick={browseUserRecipes}>Cookbook</Dropdown.Item>\n                    <Dropdown.Item onClick={toAddRecipe}>New Recipe</Dropdown.Item>\n                    <Dropdown.Item onClick={logout}>Logout</Dropdown.Item>\n                </Dropdown.Menu>\n            </Dropdown>\n        </div>\n    )\n}\n\nexport default UserDashboardMenu;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboardUser.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserProfile.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/MultiRecipeList.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/SingleRecipeList.jsx",["215"],"import React, { useEffect, useState } from 'react';\nimport { useSelector } from 'react-redux';\nimport { useLocation } from 'react-router-dom';\n\nimport RecipeController from '../../controller/RecipeController.js';\nimport { selectRecipeList, selectUserData, selectUserRecipeList } from '../../model/state/Selector.js';\nimport RecipeCard from './RecipeCard.jsx';\n\nfunction SingleRecipeList() {\n    const id = useSelector(selectUserData).id;\n    const recipes = useSelector(selectRecipeList);\n    const userRecipes = useSelector(selectUserRecipeList);\n    const path = useLocation().pathname;\n    const [list, setList] = useState(null);\n\n    useEffect(() => {\n        switch (path) {\n            case '/recipes/all':\n                setList('all')\n                return RecipeController.getRecipeList();\n            case '/recipes/user':\n                setList('user');\n                return RecipeController.getUserRecipeList(id);\n            default:\n                return;\n            }\n    }, [id]);\n    return(\n        <div>\n            {list === 'all' ? \n            <div>\n                <RecipeCard recipes={recipes} />\n            </div>\n            : list === 'user' ?\n            <div>\n                <RecipeCard recipes={userRecipes} />\n            </div>\n            :\n            null\n            }\n        </div>\n    )\n}\n\nexport default SingleRecipeList;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/helpers/functions/locationHandler.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/routes/AppRoute.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/routes/AppNavigator.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/auth/AuthError.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/formComponents/addRecipe/AddRecipeName.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/formComponents/addRecipe/AddRecipeDescription.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/formComponents/addRecipe/AddRecipeIngredients.jsx",[],["216","217"],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/formComponents/addRecipe/AddRecipeInstructions.jsx",[],["218","219"],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/formComponents/formError/RequiredField.jsx",[],["220","221"],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/helpers/functions/formFunctionHandler.js",["222","223"],"class formFunctions {\n    static convertArrToHtml(inputArr) {\n        const outputArr = inputArr.map(item => {\n            return {\n                htmlId: inputArr?.htmlId || this.generateId(),\n                name: item?.name || '',\n                recipe: item?.recipe || null\n            };\n        });\n        return outputArr;\n    };\n    static editListItem(item, value) {\n        // this sets an instruction or ingredient before it is passed to the reducer\n        return {\n            htmlId: item.htmlId,\n            name: value,\n            recipe: item.recipe\n        }\n    };\n    static setListItem(item, list) {\n        const newList = list.map(listItem => \n            listItem.htmlId !== item.htmlId ? listItem : item\n        );\n        console.log('edited list: ',newList)\n        return newList\n    };\n    static removeListItem(item, list) {\n        const filteredList = list.filter(listItem => {\n            return listItem.htmlId !== item.htmlId\n        });\n        return filteredList;\n    };\n    static generateId() {\n        return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function(c) {\n            const r = Math.random() * 16 | 0, v = c === 'x' ? r : (r & 0x3 | 0x8);\n            return v.toString(16);\n        })\n    }\n};\n\nexport default formFunctions;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/SearchRecipes.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/RecipeForm.jsx",["224","225","226","227","228"],"import React, { useEffect, useState } from 'react';\nimport { useSelector } from 'react-redux';\nimport { Form, Button, Card, CardImg, Row, Col } from 'react-bootstrap';\nimport { useParams } from 'react-router-dom';\n\nimport AddRecipeName from '../formComponents/addRecipe/AddRecipeName.jsx';\nimport AddRecipeDescription from '../formComponents/addRecipe/AddRecipeDescription.jsx';\nimport RecipeFormIngredients from './RecipeFormIngredients.jsx';\nimport RecipeFormInstructions from './RecipeFormInstructions.jsx';\n\nimport UserController from '../../controller/UserController.js';\nimport RecipeController from '../../controller/RecipeController.js';\nimport imageDefault from '../../images_static/plate-utensils.jpeg';\nimport FormHelper from '../../helpers/functions/formFunctionHandler.js';\nimport { \n    selectUser, \n    selectRecipeData, \n    selectIngredientsData, \n    selectInstructionsData,\n    selectIngredient,\n    selectDeleteIngredient,\n    selectInstruction,\n    selectDeleteInstruction \n    } from '../../model/state/Selector.js';\n\nfunction RecipeForm() {\n    const user = useSelector(selectUser);\n    const recipeData = useSelector(selectRecipeData);\n    const ingredientsData = useSelector(selectIngredientsData);\n    const instructionsData = useSelector(selectInstructionsData);\n    const { recipeId } = useParams();\n    const ingredient = useSelector(selectIngredient);\n    const deleteIngredient = useSelector(selectDeleteIngredient);\n    const instruction = useSelector(selectInstruction);\n    const deleteInstruction = useSelector(selectDeleteInstruction);\n\n    const [file, setFile] = useState('');\n    const [filename, setFilename] = useState('');\n    const [imgPreview, setImgPreview] = useState('');\n    const amountOfIngredients = ingredientsData.length || 5;\n    const amountOfInstructions = instructionsData.length || 3;\n    const [ingredients, setIngredients] = useState(FormHelper.convertArrToHtml(recipeId ? ingredientsData : Array(amountOfIngredients).fill({})))\n    const [instructions, setInstructions] = useState(FormHelper.convertArrToHtml(recipeId ? instructionsData : Array(amountOfInstructions).fill({})));\n    const [data, setData] = useState({\n        name: recipeData.name || null,\n        description: recipeData.description || null,\n        author: user.id\n    });\n\n    useEffect(() => {\n        setIngredients(FormHelper.setListItem(ingredient, ingredients))\n        setInstructions(FormHelper.setListItem(instruction, instructions))\n    }, [ingredient, instruction])\n    \n    useEffect(() => {\n        setIngredients(FormHelper.removeListItem(deleteIngredient, ingredients))\n        setInstructions(FormHelper.removeListItem(deleteInstruction, instructions))\n    }, [deleteIngredient, deleteInstruction])\n\n    const incrementRecipeIngredientsArr = () => {\n        ingredients.push({});\n        setIngredients(FormHelper.convertArrToHtml(ingredients))\n    };\n    const incrementRecipeInstructionsArr = () => {\n        instructions.push({});\n        setInstructions(FormHelper.convertArrToHtml(instructions))\n    };\n\n    const handleImage = e => {\n        setFile(e.target.files[0]);\n        setFilename(e.target.files[0].name);\n        const reader = new FileReader();\n        reader.addEventListener('load', () => {\n            setImgPreview(reader.result);\n        })\n        reader.readAsDataURL(e.target.files[0])\n    };\n    \n    const handleRecipe = e => {\n        setData({...data, [e.target.name]: e.target.value});\n    }\n    \n    const cancel = e => {\n        e.preventDefault();\n        UserController.routeToDestination('browse');\n    };\n    \n    const submitRecipe = async e => {\n        e.preventDefault();\n        const id = recipeId;\n        const recipe = new FormData();\n        recipe.append('name', data.name);\n        recipe.append('description', data.description);\n        recipe.append('author',data.author);\n        recipe.append('file', file);\n        recipe.append('filename', filename);\n        {recipeId ? \n            await RecipeController.editRecipeData({recipe, ingredients, instructions}, id) \n            : \n            await RecipeController.addRecipeData({recipe, ingredients, instructions})\n        };\n        await RecipeController.getRecipeList();\n        await RecipeController.getUserRecipeList(user.id);\n        UserController.routeToDestination('browse');\n    };\n    return(\n        <div className='create-recipe'>\n            {recipeId ? <h2>Edit {data.name}</h2> : <h2>Add New Recipe</h2>}\n            <form onSubmit={submitRecipe}>\n                <Form>\n                    <Row className='form-row'>\n                        {imgPreview ? \n                            <Form.Group as={Col}>\n                                <Row>\n                                    <Card.Img src={imgPreview} /> \n                                </Row>\n                                <Row>\n                                    <input type='file' onChange={handleImage} className='image-input' />\n                                </Row>\n                            </Form.Group>\n                        : \n                        <Form.Group as={Col}>\n                            <Row>\n                                {recipeId ? null : <h4>Select Image to Upload</h4>}\n                                <CardImg src={recipeId ? `data:image/jpeg;base64,${recipeData.image}` : imageDefault} />\n                            </Row>\n                            <Row>\n                                <input type='file' onChange={handleImage} className='image-input' />\n                            </Row>\n                        </Form.Group>}\n                        <Form.Group controlId='name' as={Col}>\n                            <Row className='input-field'>\n                                <input id='name' placeholder={recipeId ? recipeData.name : 'recipe name'} type='text' name='name' onChange={handleRecipe} />\n                            </Row>\n                            <Row className='input-field'>\n                                <textarea id='description' placeholder={recipeId ? recipeData.description : 'description'} type='text' name='description' onChange={handleRecipe} />    \n                            </Row>\n                            <Row className='input-field'>\n                                <ul>\n                                {ingredients && ingredients.map(recipeIngredient => {\n                                    return (\n                                        <li key={recipeIngredient && recipeIngredient.htmlId}>\n                                            <RecipeFormIngredients recipeIngredient={recipeIngredient} />\n                                        </li>)\n                                })}\n                                </ul>\n                                <Button xs={2} className='btn btn-primary' onClick={incrementRecipeIngredientsArr}>More Ingredients</Button>\n                            </Row>\n                            <Row className='input-field'>\n                                <ol>\n                                    {instructions && instructions.map(recipeInstruction => {\n                                        return(\n                                            <li key={recipeInstruction && recipeInstruction.htmlId}>\n                                                <RecipeFormInstructions recipeInstruction={recipeInstruction} />\n                                            </li>\n                                        )\n                                    })}\n                                </ol>\n                                <Button xs={2} className='btn btn-primary' onClick={incrementRecipeInstructionsArr}>More Instructions</Button>\n                            </Row>\n                        </Form.Group>\n                    </Row>\n                    <Form.Group as={Row} className='recipe-btn-group'>\n                        <Button xs={2} className='btn btn-secondary' onClick={cancel}>Cancel</Button>\n                        <Button xs={2} className='btn btn-primary' onClick={submitRecipe}>Submit</Button>\n                    </Form.Group>\n                </Form>\n            </form>\n        </div>\n    )\n}\n\nexport default RecipeForm;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/RecipeFormInstructions.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/RecipeFormIngredients.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/controller/FormController.js",[],{"ruleId":"229","replacedBy":"230"},{"ruleId":"231","replacedBy":"232"},{"ruleId":"233","severity":1,"message":"234","line":5,"column":8,"nodeType":"235","messageId":"236","endLine":5,"endColumn":22},{"ruleId":"233","severity":1,"message":"237","line":1,"column":16,"nodeType":"235","messageId":"236","endLine":1,"endColumn":25},{"ruleId":"233","severity":1,"message":"238","line":5,"column":8,"nodeType":"235","messageId":"236","endLine":5,"endColumn":17},{"ruleId":"229","replacedBy":"239"},{"ruleId":"231","replacedBy":"240"},{"ruleId":"233","severity":1,"message":"241","line":1,"column":17,"nodeType":"235","messageId":"236","endLine":1,"endColumn":25},{"ruleId":"233","severity":1,"message":"242","line":3,"column":20,"nodeType":"235","messageId":"236","endLine":3,"endColumn":34},{"ruleId":"243","severity":1,"message":"244","line":27,"column":8,"nodeType":"245","endLine":27,"endColumn":12,"suggestions":"246"},{"ruleId":"229","replacedBy":"247"},{"ruleId":"231","replacedBy":"248"},{"ruleId":"229","replacedBy":"249"},{"ruleId":"231","replacedBy":"250"},{"ruleId":"229",