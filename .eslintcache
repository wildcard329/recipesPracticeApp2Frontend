"errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"191","messages":"192","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/index.js",[],["193","194"],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/reportWebVitals.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/App.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserList.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/EditUser.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/CreateUser.jsx",["195"],"import React, { useState } from 'react';\nimport { useHistory } from 'react-router-dom';\nimport UserController from '../../controller/UserController';\n\nfunction CreateUser() {\n    const [user, setUser] = useState({\n        username: '',\n        password: '',\n        email: ''\n    });\n\n    const enterUser = e => {\n        e.preventDefault();\n        setUser({...user, [e.target.name]: e.target.value});\n    };\n\n    const cancel = () => {\n        UserController.routeToDestination('all users');\n    };\n\n    const addUser = async () => {\n        await UserController.addNewUser(user);\n        UserController.routeToDestination('all users');\n    };\n\n    return(\n        <div>\n            <form onSubmit={addUser}>\n                <div>\n                    <label htmlFor='username'>Username</label>\n                    <input id='username' type='text' name='username' onChange={enterUser} />\n                </div>\n                <div>\n                    <label htmlFor='password'>Password</label>\n                    <input id='password' type='text' name='password' onChange={enterUser} />\n                </div>\n                <div>\n                    <label htmlFor='email'>Email</label>\n                    <input id='email' type='text' name='email' onChange={enterUser} />\n                </div>\n                <div>\n                    <button onClick={cancel}>Cancel</button>\n                    <button onClick={addUser}>Submit</button>\n                </div>\n            </form>\n        </div>\n    )\n}\n\nexport default CreateUser;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserInfo.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserCard.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/store.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/controller/UserController.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/model/services/UserServices.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/ViewUserDetails.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/RecipeInfo.jsx",["196","197","198","199","200","201"],"import React, { useEffect } from 'react';\nimport { useSelector } from 'react-redux';\nimport { useHistory } from 'react-router-dom';\nimport { Card, Row, Col, Button } from 'react-bootstrap';\nimport { BsFillTrashFill, BsPencilSquare } from 'react-icons/bs';\n\nimport { selectRecipeData, selectIngredientsData, selectInstructionsData, selectUser } from '../../model/state/Selector.js';\nimport UserHelper from '../../helpers/functions/storageHandler.js';\nimport StorageHandler from '../../helpers/functions/storageHandler.js';\nimport RecipeController from '../../controller/RecipeController.js';\nimport FormController from '../../controller/FormController.js';\n\nfunction RecipeInfo() {\n    const recipe = useSelector(selectRecipeData);\n    const userId = UserHelper.getUserId();\n    const history = useHistory();\n    const data = recipe.image;\n    const recipeId = recipe.id || StorageHandler.getRecipeId();\n\n    // whenever the component loads, it needs recipe info\n    useEffect(async () => {\n        if (!recipe.id) {\n            await RecipeController.getRecipeData(recipeId);\n        } else {\n            StorageHandler.setRecipeId(recipeId);\n        }\n    }, []);\n\n    const clone = e => {\n        e.preventDefault();\n        console.log('cloned');\n    }\n\n    const toEditRecipe = async () => {\n        history.push(`/recipe/${recipeId}/edit`);\n    };\n\n    const deleteRecipe = async () => {\n        // await RecipeController.deleteRecipe(recipe.id)\n        // StorageHandler.removeRecipeId();\n        // history.push('/recipes/browse');\n        FormController.requestDeleteRecipe(true);\n    }\n\n    return(\n        <div className='single-recipe-card'>\n            <Row>\n                <Card.Title><h2>{recipe.name}</h2></Card.Title>\n            </Row>\n            <Row>\n                <Card.Img className='recipe-data-image' src={`data:image/jpeg;base64,${data}`} />\n                <Card.Text className='image-footer'>Posted by {recipe.author}</Card.Text>\n            </Row>\n            <Row>\n                <Col >\n                    <Card.Title>Ingredients:</Card.Title>\n                    <ul>\n                        {recipe.ingredients && recipe.ingredients.map(ingredient => {\n                            return <li className='list-item'>{ingredient.name}</li>\n                        })}\n                    </ul>\n                </Col>\n                <Col xs={8}>\n                    <Card.Title>Instructions:</Card.Title>\n                    <ul>\n                        {recipe.instructions && recipe.instructions.map(instruction => {\n                            return <li className='list-item'>{instruction.name}</li>\n                        })}\n                    </ul>\n                </Col>\n            </Row>\n            <div className='user-recipe-info'>\n            {parseInt(userId) === parseInt(recipe.author) ?\n                    <>\n                    <Button onClick={toEditRecipe} variant='outline-success'><BsPencilSquare /></Button>\n                    <Button onClick={deleteRecipe} variant='outline-danger'><BsFillTrashFill /></Button>\n                    </>\n                    :\n                    <Button onClick={clone}>Add to Cookbook</Button>\n                }\n            </div>\n        </div>\n    )\n}\n\nexport default RecipeInfo;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/RecipeCard.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/controller/RecipeController.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/model/services/RecipeServices.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/model/state/reducer.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/model/state/Action.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/model/state/Selector.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/routes/PrivateRoute.jsx",["202"],"import React, {Component} from 'react';\nimport { Route, Redirect } from 'react-router-dom';\n\nconst PrivateRoute = ({ component: Component, ...rest}) => {\n    return(\n        <Route\n            {...rest}\n            render={props => {\n                if (localStorage.getItem('token')) {\n                    return <Component {...props} />;\n                } else {\n                    return <Redirect to='/auth/login' />;\n                }\n            }}\n        />\n    )\n};\n\nexport default PrivateRoute;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/auth/UserRegister.jsx",["203","204","205","206","207","208","209"],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/auth/UserLogin.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/helpers/utils/axiosWithAuth.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboard.jsx",["210","211","212","213","214","215"],"import React, { useEffect, useState } from 'react';\nimport { useLocation, useHistory } from 'react-router-dom';\nimport { useSelector } from 'react-redux';\nimport jwt_decode from 'jwt-decode';\n\nimport { selectUser, selectToken } from '../../model/state/Selector.js';\nimport UserDashboardDisplayUser from './UserDashboardDisplay.jsx';\nimport UserDashboardLogoutDsiplay from './UserDashboardLogoutDsiplay.jsx';\nimport UserHelper from '../../helpers/functions/storageHandler.js';\nimport NavHelper from '../../helpers/functions/locationHandler.js';\nimport UserController from '../../controller/UserController.js';\n\nfunction UserDashboard() {\n    const [display, setDisplay] = useState(false);\n    const token = useSelector(selectToken);\n    const id = UserHelper.getUserId();\n\n    useEffect(() => {\n        token || id ?\n            setDisplay(true)\n        :\n            setDisplay(false)\n    }, [token, id]);\n\n    return(\n        <div>\n            <div className={display ? 'none' : ''}>\n                <UserDashboardLogoutDsiplay />\n            </div>\n            <div className={display ? '' : 'none'}>\n                <UserDashboardDisplayUser />\n            </div>\n        </div>\n    )\n}\n\nexport default UserDashboard;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboardDisplay.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboardLogoutDsiplay.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/helpers/functions/storageHandler.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboardMenu.jsx",["216","217","218"],"import React, { useState } from 'react';\nimport { useSelector } from 'react-redux';\nimport { Dropdown, DropdownButton } from 'react-bootstrap';\n\nimport UserController from '../../controller/UserController';\nimport UserHelper from '../../helpers/functions/storageHandler.js';\nimport { selectUser, selectUserData } from '../../model/state/Selector.js';\nimport { useHistory } from 'react-router-dom';\n\nfunction UserDashboardMenu() {\n    const history = useHistory();\n    const userId = UserHelper.getUserId();\n    const user = useSelector(selectUserData);\n\n    const viewProfile = () => {\n        history.push('/user/profile');\n    }\n\n    const browseRecipes = () => {\n        history.push('/recipes/browse');\n    }\n\n    const browseUserRecipes = async () => {\n        await UserController.getUserData(userId);\n        history.push('/recipes/user');\n    }\n\n    const toAddRecipe = () => {\n        history.push('/recipes/add');\n    };\n\n    const logout = async () => {\n        UserHelper.removeTokenSession();\n        UserController.logoutUser(user);\n        history.push('/auth/login');\n    };\n\n    return(\n        <div className='user-dashboard-left'>\n            <Dropdown>\n                <Dropdown.Toggle variant=\"primary\" id=\"dropdown-basic\">\n                    Menu\n                </Dropdown.Toggle>\n                <Dropdown.Menu>\n                    <Dropdown.Item onClick={viewProfile}>Profile</Dropdown.Item>\n                    <Dropdown.Item onClick={browseRecipes}>Browse</Dropdown.Item>\n                    <Dropdown.Item onClick={browseUserRecipes}>Cookbook</Dropdown.Item>\n                    <Dropdown.Item onClick={toAddRecipe}>New Recipe</Dropdown.Item>\n                    <Dropdown.Item onClick={logout}>Logout</Dropdown.Item>\n                </Dropdown.Menu>\n            </Dropdown>\n        </div>\n    )\n}\n\nexport default UserDashboardMenu;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserDashboardUser.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/users/UserProfile.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/MultiRecipeList.jsx",["219"],"import React, { useEffect } from \"react\";\nimport { useSelector } from 'react-redux';\nimport { useHistory } from 'react-router-dom';\n\nimport RecipeController from '../../controller/RecipeController.js';\nimport UserController from \"../../controller/UserController.js\";\nimport { selectSampleRecipes } from '../../model/state/Selector.js';\nimport UserHelper from '../../helpers/functions/storageHandler.js';\n\nimport RecipeMapper from \"./RecipeMapper.jsx\";\n\nfunction RecipeList() {\n    const history = useHistory();\n    const userId = UserHelper.getUserId();\n    const samples = useSelector(selectSampleRecipes);\n    const filters = samples && samples[0];\n    const recipes = samples && samples[1];\n    const userRecipes = samples && samples[2];\n    const filter1 = samples && samples[3];\n    const filter2 = samples && samples[4];\n    const filter3 = samples && samples[5];\n   \n    const setUserRecipes = () => {\n        UserController.getUserData(userId);\n    };\n    \n    const toAllRecipes = () => {\n        history.push('/recipes/all');\n    }\n    \n    const toRecipeList = () => {\n        history.push('/recipes/user');\n    }\n    \n    useEffect(async () => {\n        await RecipeController.browseRecipes(userId);\n    }, [userId]);\n\n    return(\n        <div>\n            <div className='recipe-lists'>\n                <h2 className='list-header' onClick={toAllRecipes}>Browse all recipes</h2>\n                <RecipeMapper recipes={recipes} />\n            </div>\n            <div className='recipe-lists' onClick={setUserRecipes}>\n                <h2 className='list-header' onClick={toRecipeList}>Your Recipes</h2>\n                <RecipeMapper recipes={userRecipes} />\n            </div>\n            <div className='recipe-lists' onClick={null}>\n                <h2 className='list-header' onClick={null}>Try {filters && filters[0]} Recipes</h2>\n                <RecipeMapper recipes={filter1} />\n            </div>\n            <div className='recipe-lists' onClick={null}>\n                <h2 className='list-header' onClick={null}>Try {filters && filters[1]} Recipes</h2>\n                <RecipeMapper recipes={filter2} />\n            </div>\n            <div className='recipe-lists' onClick={null}>\n                <h2 className='list-header' onClick={null}>Try {filters && filters[2]} Recipes</h2>\n                <RecipeMapper recipes={filter3} />\n            </div>\n        </div>\n    )\n}\n\nexport default RecipeList;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/SingleRecipeList.jsx",["220","221"],"import React, { useEffect, useState } from 'react';\nimport { useSelector } from 'react-redux';\nimport { useLocation } from 'react-router-dom';\n\nimport RecipeController from '../../controller/RecipeController.js';\nimport { selectRecipeList, selectUserData, selectUserRecipeList } from '../../model/state/Selector.js';\nimport StorageHandler from '../../helpers/functions/storageHandler.js';\n\nimport RecipeMapper from './RecipeMapper.jsx';\n\nfunction SingleRecipeList() {\n    const id = useSelector(selectUserData).id || StorageHandler.getUserId();\n    const recipes = useSelector(selectRecipeList);\n    const userRecipes = useSelector(selectUserRecipeList);\n    const path = useLocation().pathname;\n    const [list, setList] = useState(null);\n    \n    useEffect(async () => {\n        switch (path) {\n            case '/recipes/all':\n                await RecipeController.getRecipeList();\n                setList(recipes)\n                break;\n            case '/recipes/user':\n                await RecipeController.getUserRecipeList(id);\n                setList(userRecipes);\n                break;    \n            default:\n                return;\n            }\n        }, [id, list]);\n        \n    return(\n        <div className='single-list'>\n            <RecipeMapper recipes={list} />\n        </div>\n    )\n}\n\nexport default SingleRecipeList;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/helpers/functions/locationHandler.js",[],["222","223"],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/routes/AppRoute.jsx",["224","225","226"],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/auth/AuthError.jsx",[],["227","228"],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/helpers/functions/formFunctionHandler.js",["229","230"],"class formFunctions {\n    static async convertArrToHtml(inputArr) {\n        // this maps an array to html objects\n        // console.log('input: ',inputArr)\n        const outputArr = await inputArr?.map(item => {\n            return {\n                htmlId: inputArr?.htmlId || this.generateId(),\n                name: item?.name || '',\n                recipe: item?.recipe || null,\n                serverId: item?.id || null\n            };\n        });\n        // console.log('output: ',outputArr);\n        return outputArr;\n    };\n    static editListItem(item, value) {\n        // this sets an instruction or ingredient before it is passed to the reducer\n        return {\n            htmlId: item.htmlId,\n            name: value,\n            recipe: item.recipe\n        }\n    };\n    static setListItem(item, list) {\n        // this enables editing of the html, which if set in the component would have to use every key from the ingredient object when only the name key is needed\n        const newList = list?.map(listItem => \n            listItem.htmlId !== item.htmlId ? listItem : item\n        );\n        return newList\n    };\n    static removeListItem(item, list) {\n        // this removes a list element from the html and from state\n        const filteredList = list?.filter(listItem => {\n            return listItem.htmlId !== item.htmlId\n        });\n        return filteredList;\n    };\n    static generateId() {\n        // this generates a unique id for html elements\n        return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function(c) {\n            const r = Math.random() * 16 | 0, v = c === 'x' ? r : (r & 0x3 | 0x8);\n            return v.toString(16);\n        })\n    };\n};\n\nexport default formFunctions;\n","/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/SearchRecipes.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/RecipeForm.jsx",["231","232","233","234","235","236","237","238"],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/RecipeFormInstructions.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/RecipeFormIngredients.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/controller/FormController.js",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/RecipeMapper.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/routes/AppNavBar.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/FormModal.jsx",[],"/home/wildcard/Programming/Javascript/React/recipes_app_frontend2/src/view/recipes/DeleteRecipeConfirmation.jsx",[],{"ruleId":"239","replacedBy":"240"},{"ruleId":"241","replacedBy":"242"},{"ruleId":"243","severity":1,"message":"244","line":2,"column":10,"nodeType":"245","messageId":"246","endLine":2,"endColumn":20},{"ruleId":"243","severity":1,"message":"247","line":7,"column":28,"nodeType":"245","messageId":"246","endLine":7,"endColumn":49},{"ruleId":"243","severity":1,"message":"248","line":7,"column":51,"nodeType":"245","messageId":"246","endLine":7,"endColumn":73},{"ruleId":"243","severity":1,"message":"249","line":7,"column":75,"nodeType":"245","messageId":"246